// This code is inserted into a separate module on enjin, where code is put into every head tag of every page.
// This code snippet is needed to start tracking your website to link to your GA account.
// This snippet can be found in your Google Analytics Web Dashboard under Admin>Property>Tracking Info>Tracking code

<script>
  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
  })(window,document,'script','//www.google-analytics.com/analytics.js','ga');


// put your tracking ID after 'create',

ga('create', 'UA-66119136-1', 'auto');


ga('send', 'pageview');


</script>

